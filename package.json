{
    "name": "react-router-chart",
    "version": "0.0.9",
    "description": "Static route mapping for React Router",
    "main": "./lib/main.js",
    "scripts": {
        "test": "jest",
        "precommit": "lint-staged",
        "build": "webpack --mode=production",
        "watch": "webpack --mode=development --watch"
    },
    "lint-staged": {
        "linters": {
            "*.js": [
                "eslint",
                "jest --bail --findRelatedTests"
            ]
        },
        "ignore": [
            "**/lib/*"
        ]
    },
    "jest": {
        "testPathIgnorePatterns": [
            "./artifacts/",
            "./node_modules/"
        ],
        "setupTestFrameworkScriptFile": "./config/setupTests.js",
        "coverageDirectory": "./artifacts/coverage"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/iamogbz/react-router-chart.git"
    },
    "keywords": [
        "react",
        "router",
        "react-router",
        "javascript",
        "es6",
        "nodejs",
        "chart",
        "graph",
        "cartographer"
    ],
    "author": "iamogbz",
    "license": "GPL-3.0",
    "bugs": {
        "url": "https://github.com/iamogbz/react-router-chart/issues"
    },
    "homepage": "https://github.com/iamogbz/react-router-chart#readme",
    "dependencies": {
        "react": "^16.6.0",
        "react-router": "^5.0.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.3.4",
        "@babel/preset-env": "^7.3.4",
        "@babel/preset-react": "^7.0.0",
        "babel-core": "^7.0.0-bridge.0",
        "babel-eslint": "^10.0.1",
        "babel-loader": "^8.0.5",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "coveralls": "^3.0.3",
        "enzyme": "^3.9.0",
        "enzyme-adapter-react-16": "^1.10.0",
        "eslint": "^5.16.0",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-config-prettier": "^4.1.0",
        "eslint-plugin-import": "^2.16.0",
        "eslint-plugin-jsx-a11y": "^6.2.1",
        "eslint-plugin-prettier": "^3.0.1",
        "eslint-plugin-react": "^7.12.4",
        "jest": "^24.1.0",
        "lint-staged": "^8.1.4",
        "prettier": "^1.14.3",
        "react-dom": "^16.8.3",
        "rxjs": "^6.4.0",
        "webpack": "^4.29.6",
        "webpack-cli": "^3.2.3"
    }
}
