{
    "name": "react-router-chart",
    "version": "0.0.0",
    "description": "Static route mapping for React Router",
    "author": "iamogbz",
    "license": "GPL-3.0",
    "main": "./main.js",
    "homepage": "https://github.com/iamogbz/react-router-chart#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/iamogbz/react-router-chart.git"
    },
    "bugs": {
        "url": "https://github.com/iamogbz/react-router-chart/issues"
    },
    "keywords": [
        "react",
        "router",
        "react-router",
        "javascript",
        "es6",
        "nodejs",
        "chart",
        "graph",
        "cartographer"
    ],
    "engines": {
        "node": ">=10.4.0",
        "npm": ">=6.1.0"
    },
    "scripts": {
        "commit": "git-cz",
        "commitlint": "commitlint-travis",
        "test": "jest",
        "build": "webpack --mode=production",
        "watch": "webpack --mode=development --watch",
        "release": "semantic-release"
    },
    "release": {
        "plugins": [
            "@semantic-release/commit-analyzer",
            "@semantic-release/release-notes-generator",
            "@semantic-release/github",
            [
                "@semantic-release/npm",
                {
                    "pkgRoot": "./lib"
                }
            ]
        ]
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "lint-staged": {
        "linters": {
            "*.js": [
                "eslint"
            ],
            "*.ts": [
                "tslint"
            ],
            "*.{js,ts}": [
                "jest --bail --findRelatedTests"
            ]
        },
        "ignore": [
            "**/lib/*"
        ]
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
            "pre-commit": "lint-staged"
        }
    },
    "jest": {
        "testPathIgnorePatterns": [
            "./artifacts/",
            "./node_modules/"
        ],
        "setupTestFrameworkScriptFile": "./config/setupTests.ts",
        "coverageDirectory": "./artifacts/coverage"
    },
    "peerDependencies": {
        "react": "^16.0.0",
        "react-router": "^4.0.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.3.4",
        "@babel/plugin-proposal-class-properties": "^7.4.0",
        "@babel/preset-env": "^7.3.4",
        "@babel/preset-react": "^7.0.0",
        "@babel/preset-typescript": "^7.3.3",
        "@commitlint/cli": "^7.5.2",
        "@commitlint/config-conventional": "^7.5.0",
        "@commitlint/travis-cli": "^7.5.2",
        "@types/copy-webpack-plugin": "^4.4.3",
        "@types/enzyme": "^3.9.1",
        "@types/jest": "^24.0.9",
        "@types/node": "^11.12.2",
        "@types/react": "^16.8.14",
        "@types/react-router": "^4.4.5",
        "@types/webpack": "^4.4.25",
        "@typescript-eslint/eslint-plugin": "^1.6.0",
        "@typescript-eslint/parser": "^1.6.0",
        "acorn": "^6.1.1",
        "babel-core": "^7.0.0-bridge.0",
        "babel-eslint": "^10.0.1",
        "babel-loader": "^8.0.5",
        "commitizen": "^3.0.7",
        "copy-webpack-plugin": "^5.0.2",
        "coveralls": "^3.0.3",
        "cz-conventional-changelog": "^2.1.0",
        "enzyme": "^3.9.0",
        "enzyme-adapter-react-16": "^1.10.0",
        "eslint": "^5.16.0",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-config-prettier": "^4.1.0",
        "eslint-plugin-import": "^2.16.0",
        "eslint-plugin-jsx-a11y": "^6.2.1",
        "eslint-plugin-prettier": "^3.0.1",
        "eslint-plugin-react": "^7.12.4",
        "husky": "^1.3.1",
        "jest": "^24.1.0",
        "lint-staged": "^8.1.4",
        "prettier": "^1.16.4",
        "react": "^16.8.6",
        "react-dom": "^16.8.3",
        "react-router": "^4.3.1",
        "rxjs": "^6.4.0",
        "semantic-release": "^15.13.12",
        "ts-jest": "^24.0.0",
        "ts-node": "^8.0.2",
        "typescript": "^3.3.3333",
        "webpack": "^4.29.6",
        "webpack-cli": "^3.2.3"
    }
}
