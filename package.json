{
  "name": "react-router-chart",
  "version": "0.0.7",
  "description": "Static route mapping for React Router",
  "main": "./lib/main.js",
  "scripts": {
    "test": "jest",
    "precommit": "lint-staged",
    "build": "webpack"
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "eslint",
        "jest --bail --findRelatedTests"
      ]
    },
    "ignore": [
      "**/lib/*"
    ]
  },
  "jest": {
    "testPathIgnorePatterns": [
      "./artifacts/",
      "./node_modules/"
    ],
    "coverageDirectory": "./artifacts/coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iamogbz/react-router-chart.git"
  },
  "keywords": [
    "react",
    "router",
    "react-router",
    "javascript",
    "es6",
    "nodejs",
    "chart",
    "graph",
    "cartographer"
  ],
  "author": "iamogbz",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/iamogbz/react-router-chart/issues"
  },
  "homepage": "https://github.com/iamogbz/react-router-chart#readme",
  "dependencies": {
    "react": "^16.4.1",
    "react-router": "^4.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "coveralls": "^3.0.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0",
    "jest": "^23.4.1",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.7",
    "rxjs": "^6.2.2",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0"
  }
}
